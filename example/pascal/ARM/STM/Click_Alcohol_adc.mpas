unit Click_Alcohol_adc;

uses Click_Alcohol_types;

procedure alcohol_adcInit(); 
function  alcohol_readADC() :  uint32_t; 

implementation

procedure alcohol_adcInit(); 
begin

    ADC1_Init(); 
    ADC_Set_Input_Channel(_ADC_CHANNEL_3); 

end;

function  alcohol_readADC() :  uint32_t; 
var
    readADC : uint32_t;
begin

    readADC := ADC1_Read(_ADC_CHANNEL_3); 
    result := readADC; 

end;

end.